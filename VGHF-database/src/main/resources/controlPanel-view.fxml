<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <AnchorPane fx:id="gamesTab" prefHeight="400.0" prefWidth="600.0">
        <children>
            <Button fx:id="refreshButton" text="Refresh" prefWidth="100" AnchorPane.topAnchor="10" AnchorPane.rightAnchor="100"/>
            <TextField fx:id="gameSearchText" promptText="Search..." AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="200"/>
            <TilePane fx:id="gamesTableView" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="50"/>
        </children>
    </AnchorPane>

</AnchorPane>
